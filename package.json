{
  "name": "courseapp",
  "version": "1.0.0",
  "description": "Courses Catalog",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.kabudev.curseapp",
    "productName": "curseApp",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "index.html",
      "package.json",
      "node_modules/**/*",
      "assets/**/*",
      "analytics.js",
      "renderer.js",
      "assets/**",
      "castarsdk/**",
      "courses-area/**",
      "analytics.js",
      "analytics-conf.json",
      "preload.js",
      "course-detail.html",
      "course-detail-renderer.js",
      "courses.json",
      "icon.ico",
      "index.html",
      "json-timestamp.json",
      "load-search-bar.js",
      "load-sidebar.js",
      "my-courses-renderer.js",
      "my-courses.html",
      "notification.js",
      "package-lock.json",
      "plataforms.json",
      "renderer.js",
      "search-bar.html",
      "sidebar.html",
      "suport.html"
    ],
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility"
    },
    "publish": {
      "provider": "github",
      "owner": "josecantero",
      "repo": "https://github.com/josecantero/curseApp"
    }
  },
  "dependencies": {
    "axios": "1.12.2",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "ffi-rs": "1.3.0",
    "node-fetch": "2.7.0",
    "rpmbuild": "^0.0.23",
    "sqlite3": "5.1.7",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "electron": "^38.1.2",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "3.2.9"
  },
  "author": "MyCourseApp",
  "license": "MIT"
}

{
  "name": "courseapp",
  "version": "1.0.80",
  "description": "Courses Catalog",
  "main": "src/main/main.js",
  "resolutions": {
    "uglify-js": "^3.17.4"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.kabudev.curseapp",
    "productName": "curseApp",
    "directories": {
      "output": "dist"
    },
    "asar": true,
    "asarUnpack": [
      "**/castarsdk/linux-sdk/**/*",
      "**/castarsdk/win-sdk/**/*",
      "**/node_modules/koffi/**/*"
    ],
    "files": [
      "src/**/*",
      "assets/**/*",
      "data/**/*",
      "config/**/*",
      "castarsdk/**/*",
      "package.json",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/images/icon.png"
    },
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "icon": "assets/images/icon.png"
    },
    "publish": {
      "provider": "github",
      "owner": "josecantero",
      "repo": "curseApp"
    }
  },
  "dependencies": {
    "axios": "1.12.2",
    "build": "^0.1.4",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "ffi-rs": "1.3.0",
    "koffi": "^2.14.1",
    "node-fetch": "2.7.0",
    "rpmbuild": "^0.0.23",
    "sqlite3": "5.1.7",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@electron/rebuild": "^4.0.1",
    "electron": "^39.2.6",
    "electron-builder": "^26.1.0",
    "npm-force-resolutions": "^0.0.10"
  },
  "author": "MyCourseApp",
  "license": "MIT"
}